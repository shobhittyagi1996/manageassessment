<mvc:View
    controllerName="com.sap.manageassessment.controller.View2"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
>
    <NavContainer id="wizardNavContainer">
        <pages>
            <Page
                id="wizardContentPage"
                showHeader="false"
            >
                <content>
                    <Wizard
                        id="CreateProductWizard"
                        class="sapUiResponsivePadding--header sapUiResponsivePadding--content"
                        complete="wizardCompletedHandler"
                    >
                        <WizardStep
                            id="ProductTypeStep"
                            title="Template Details"
                            validated="true"
                        >
                            <form:SimpleForm
                                id="_IDGenSimpleForm1"
                                editable="true"
                                layout="ResponsiveGridLayout"
                            >
                                <Label
                                    id="_IDGenLabel1"
                                    text="Title"
                                    required="true"
                                />
                                <Input
                                    id="inp1"
                                    value=""
                                />
                                <Label
                                    id="_IDGenLabel2"
                                    text="Assessment Type"
                                    required="true"
                                />
                                <Select
                                    id="Select1112"
                                    change="onSelectChange"
                                    items="{sModel>/}"
                                >
                                    <core:Item
                                        id="_IDGenItem1245"
                                        key="{sModel>text}"
                                        text="{sModel>text}"
                                    />
                                </Select>
                            </form:SimpleForm>
                        </WizardStep>
                        <WizardStep
                            id="selectinfo"
                            validated="true"
                            title="Select Questions for Assessment"
                            activate="additionalInfoValidation"
                        >
                            <Table
                                id="idSupplierTable"
                                items="{uModel>/}"
                                autoPopinMode="true"
                            >
                                <headerToolbar>
                                    <Toolbar id="_IDGenToolbar1">
                                        <HBox
                                            id="_IDGenHBox1"
                                            alignItems="Center"
                                            width="50%"
                                            justifyContent="SpaceAround"
                                        />
                                    </Toolbar>
                                </headerToolbar>
                                <columns>
                                    <Column id="_IDGenColumn1">
                                        <Text
                                            id="_IDGenText2"
                                            text="Questions"
                                        />
                                    </Column>
                                    <Column id="_IDGenColumn2">
                                        <Text
                                            id="_IDGenText3"
                                            text="Select"
                                        />
                                    </Column>
                                    <Column id="_IDGenColumn21">
                                        <Text
                                            id="_IDGenText12"
                                            text="Response Type"
                                        />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem1">
                                        <cells>
                                            <Text
                                                id="_IDGenText6"
                                                text="{uModel>question}"
                                            />
                                            <CheckBox
                                                id="_IDGenCheckBox1"
                                                text=""
                                            />
                                            <Select
                                                id="Select112"
                                                change="onSelectChangeKeyFigure"
                                                items="{fModel>/}"
                                                width="8rem"
                                                liveChange=".onInputFocus"
                                            >
                                                <items>
                                                    <core:Item
                                                        id="_IDGenItem12345"
                                                        key="{fModel>responsetype}"
                                                        text="{fModel>responsetype}"
                                                    />
                                                </items>
                                            </Select>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                            <Toolbar id="_IDGenToolbar2">
                                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                                <Button
                                    id="_IDGenButton1"
                                    text="Save"
                                    type="Emphasized"
                                />
                            </Toolbar>
                        </WizardStep>
                    </Wizard>
                </content>
            </Page>
        </pages>
    </NavContainer>
</mvc:View>
